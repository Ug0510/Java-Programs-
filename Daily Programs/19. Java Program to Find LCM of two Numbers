// Java Program to Find LCM of two Numbers

class LCM {
    public static void main(String[] args) {
        int n1 = 3;
        int n2 = 4;
        
        int lcm = (n1>n2)?n1:n2;
        
        while(true)
        {
            if(lcm%n1 == 0 && lcm%n2 == 0)
                break;
            lcm++;
        }
        System.out.println("Lcm of "+n1+" and "+n2+" is "+lcm);
    }
}
