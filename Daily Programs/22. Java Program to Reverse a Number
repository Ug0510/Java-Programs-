// Java Program to Reverse a Number

class ReverseNumber {
    public static void main(String[] args) {
        int num = 345678;
        int r = 0;
        while(num!=0)
        {
            int a = num % 10;
            r = r * 10 + a;
            num/=10;
        }
        System.out.println("Reverse of Number is "+r);
    }
}
