// Java Program to Find Largest Element of an Array

class ArrayLargest{
    public static void main(String[] args) {
        int arr[] = {2,3,5,6,7,1,9};
        int max = 0;
        for(int i=0; i<arr.length; i++)
        {
            if(arr[i] > max)
                max = arr[i];
        }
        System.out.println("Largest element of array is "+ max);
        
    }
}
