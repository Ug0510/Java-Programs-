//Java Program to Display Fibonacci Series

class FibSeries{
    public static void main(String args[])
    {
        int n = 8;
        int a = 0;
        int b = 1;
        int c;
        System.out.println("Fibonacci Series:");
        System.out.print(a+" "+b+" ");
        for(int i = 3; i<=n; i++)
        {
            c = a + b;
            a = b;
            b = c;
            System.out.print(c+" ");
        }
        
    }
}
